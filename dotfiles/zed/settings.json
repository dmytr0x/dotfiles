{
  "auto_update": false,
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "session": {
    "restore_unsaved_buffers": true,
    "trust_all_worktrees": false,
  },
  "agent": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-haiku-4-5"
    },
    "enable_feedback": false,
    "always_allow_tool_actions": false,
    "model_parameters": [],
  },
  "ui_font_size": 14.0,
  "ui_font_family": ".ZedMono",
  "buffer_font_size": 13.0,
  "buffer_font_family": ".ZedMono",
  "tab_bar": {
    "show": false,
  },
  "theme": "Gruvbox Dark Hard",
  "theme_overrides": {
    "Gruvbox Dark Hard": {
      "syntax": {
        // Reduce the prominence of certain captures in the code
        "type": { "color": "#5f5a51ff" },
        "comment": { "color": "#5f5a51ff" },
      },
    },
  },
  "base_keymap": "VSCode",
}
